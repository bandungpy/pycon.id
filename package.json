{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm prisma generate && pnpm remix build",
    "dev": "remix dev",
    "typecheck": "tsc",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "lint": "eslint \"app/**/*.+(ts|tsx)\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier app --check",
    "prettier:fix": "prettier app --write",
    "format-code": "npm run prettier:fix & npm run lint:fix"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/vercel": "^1.18.1",
    "@vercel/node": "^2.10.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@remix-run/serve": "^1.18.1",
    "@types/node": "20.4.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "prisma": "5.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}